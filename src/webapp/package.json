{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run lint && vue-cli-service build",
    "build-dev": "vue-cli-service build --mode=development --watch",
    "lint": "vue-cli-service lint",
    "apollo": "vue-cli-service apollo:dev --generate-schema",
    "apollo:schema:generate": "vue-cli-service apollo:schema:generate",
    "apollo:schema:publish": "vue-cli-service apollo:schema:publish",
    "apollo:start": "vue-cli-service apollo:start",
    "fetch-schema": "curl -X POST -H \"Content-Type: application/json\" --data '{ \"query\": \"{ __schema { types { kind, name, possibleTypes { name } } } }\" }' $VUE_APP_GRAPHQL_CLI_HTTP | sed 's/\"possibleTypes\":null/\"possibleTypes\":[]/g' > src/schema.json"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^23.1.0",
    "@ckeditor/ckeditor5-vue2": "^1.0.5",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fullcalendar/bootstrap": "^4.4.2",
    "@fullcalendar/core": "^4.4.2",
    "@fullcalendar/daygrid": "^4.4.2",
    "@fullcalendar/timegrid": "^4.4.2",
    "@fullcalendar/vue": "^4.4.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "core-js": "^3.4.4",
    "element-ui": "^2.13.0",
    "graphql-type-json": "^0.2.1",
    "izitoast": "^1.4.0",
    "jquery": "^3.4.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "popper.js": "^1.16.1",
    "register-service-worker": "^1.6.2",
    "tippy.js": "^6.2.3",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-autosuggest": "^2.0.4",
    "vue-class-component": "^7.0.2",
    "vue-form-wizard": "^0.8.4",
    "vue-markdown": "^2.2.4",
    "vue-phone-number-input": "^1.1.4",
    "vue-property-decorator": "^8.3.0",
    "vue-rate": "^2.4.0",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.1.3",
    "vue-spinners": "^1.0.2",
    "vue2-perfect-scrollbar": "^1.3.0",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-paginate": "^2.1.0"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.3",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-pwa": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-upload-client": "^12.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "graphql-tag": "^2.9.0",
    "node-sass": "^4.13.1",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "typescript": "~3.5.3",
    "vue-cli-plugin-apollo": "^0.21.3",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.5"
  }
